{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "fit/beautiful-salat",
  "version": "0.1.0",
  "title": "Beautiful Salat",
  "category": "widgets",
  "icon": "smiley",
  "description": "Prayer times with unlimited color combinations, offering daily, weekly, monthly, and yearly tables.",
  "supports": {
    "html": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Prayer Times"
    },
    "maxWidth": {
      "type": "number"
    },
    "backgroundColor": {
      "type": "string"
    },
    "titleTextColor": {
      "type": "string"
    },
    "showHeader": {
      "type": "boolean",
      "default": true
    },
    "salatSettings": {
      "type": "object",
      "default": {
        "method": "MWL",
        "country": "US",
        "city": "Los Angeles",
        "timings": {
          "Fajr": 0
        }
      }
    }
  },
  "example": {
    "attributes": {
      "title": "Prayer Times"
    }
  },
  "textdomain": "beautiful-salat",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/style-index.css",
  "style": "file:./build/style-index.css"
}
