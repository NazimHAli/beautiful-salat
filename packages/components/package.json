{
  "name": "@beautiful-salat/components",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "files": [
    "build"
  ],
  "main": "./build/index.es.js",
  "module": "./build/index.es.js",
  "exports": {
    ".": {
      "import": "./build/index.es.js",
      "require": "./build/index.umd.js"
    },
    "./package.json": "./package.json",
    "./style.css": "./build/style.css"
  },
  "scripts": {
    "build:package": "vite build --emptyOutDir",
    "build:minify": "esbuild ./build/index.es.js --bundle --splitting --format=esm --minify --allow-overwrite --outdir=./build",
    "sveltecheck": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite",
    "test": "yarn run test:tsc",
    "test:jest": "yarn run jest",
    "test:tsc": "tsc --noEmit",
    "serve": "vite preview --port 8080"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.31",
    "@testing-library/dom": "^8.20.1",
    "@tsconfig/svelte": "^3.0.0",
    "@types/cssnano": "^4.0.1",
    "@types/postcss-import": "^14.0.3",
    "@types/tailwindcss": "^3.0.11",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.19",
    "cssnano": "^5.1.15",
    "esbuild": "^0.21.5",
    "postcss": "^8.4.38",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-sass": "^0.5.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.77.6",
    "stylelint": "^14.16.1",
    "svelte": "^3.59.2",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^4.9.5",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^0.9.10"
  },
  "peerDependencies": {
    "jest": "*"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "@beautiful-salat/js-lib": "workspace:^"
  }
}
